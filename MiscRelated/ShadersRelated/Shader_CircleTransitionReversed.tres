[gd_resource type="Shader" format=2]

[resource]
code = "
shader_type canvas_item;

uniform float circle_size : hint_range(0, 1.05) = 0.5f;

void fragment(){
	vec4 curr_color = texture(TEXTURE,UV);
	if (distance(vec2(0.5,0.5), UV) < circle_size){
		COLOR = vec4(1,0,0,0);
	}
	else {
		COLOR = curr_color;
	}
}"
